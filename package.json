{
  "name": "data-platform",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.20.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:api": "pnpm --filter @dp/api dev",
